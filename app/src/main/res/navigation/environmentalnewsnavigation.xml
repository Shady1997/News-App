<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/environmentalnewsnavigation"
    app:startDestination="@id/websitesListFragment">

    <fragment
        android:id="@+id/websitesListFragment"
        android:name="com.graduationproject.egyptnews.views.fragments.environmentalNewsFreagments.WebsitesListFragment"
        android:label="fragment_websites_list"
        tools:layout="@layout/fragment_websites_list" >
        <action
            android:id="@+id/action_websitesListFragment_to_environmentalNewsFragment"
            app:destination="@id/environmentalNewsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/environmentalNewsFragment"
        android:name="com.graduationproject.egyptnews.views.fragments.environmentalNewsFreagments.EnvironmentalNewsFragment"
        android:label="fragment_environmental_news"
        tools:layout="@layout/fragment_environmental_news" />
</navigation>